{
  "name": "calculator",
  "productName": "Calculator-app",
  "version": "1.0.8",
  "description": "un mic proiect",
  "main": "src/index.js",
  "icon": "src/icons/iconita.ico",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build": "electron-builder",
    "deploy": "electron-builder build --win --publish always"
  },
  "keywords": [],
  "author": "Lucas",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lucas4406/my-new-app.git"
  },
  "build": {
    "appId": "Calculator",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/icons/iconita.ico"
    },
    "publish": {
      "provider": "github",
      "vPrefixedTagName": true
    },
    "nsis": {
      "oneClick": false,
      "installerIcon": "src/icons/iconita.ico",
      "uninstallerIcon": "src/icons/iconita.ico",
      "uninstallDisplayName": "Calculator-uninstaller",
      "license": "src/license.md",
      "allowToChangeInstallationDirectory": true
    }
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "src/icons/iconita.ico"
      },
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "Lucas4406",
              "name": "my-new-app"
            }
          }
        }
      ],
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "my_new_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@electron-forge/publisher-github": "^6.0.0-beta.64",
    "electron": "^19.0.6",
    "electron-builder": "^23.0.3"
  }
}
